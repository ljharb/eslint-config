{
	"name": "@ljharb/eslint-config",
	"version": "6.0.0",
	"author": "Jordan Harband",
	"description": "My shareable eslint config",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"prepublish": "safe-publish-latest && in-install || (npm test && eslint-find-rules --unused)",
		"test": "npm run --silent tests-only",
		"posttest": "npm run --silent security",
		"tests-only": "eslint index.js",
		"security": "nsp check"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/eslint-config.git"
	},
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"dependencies": {},
	"devDependencies": {
		"eslint": "^3.0.0",
		"eslint-find-rules": "^1.11.0",
		"nsp": "^2.5.0",
		"replace": "^0.3.0",
		"semver": "^5.2.0",
		"in-publish": "^2.0.0",
		"safe-publish-latest": "^1.0.0"
	},
	"peerDependencies": {
		"eslint": "^3.0.0"
	},
	"engines": {
		"node": ">= 4"
	},
	"publishConfig": {
		"access": "public"
	}
}
